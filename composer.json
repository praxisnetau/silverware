{
  "name": "silverware/silverware",
  "type": "silverstripe-module",
  "description": "SilverWare Component Framework.",
  "homepage": "https://github.com/praxisnetau/silverware",
  "keywords": [
    "silverware",
    "component",
    "framework",
    "silverstripe"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Colin Tucker",
      "role": "Developer",
      "email": "colin@praxis.net.au",
      "homepage": "http://www.praxis.net.au"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "embed/embed": "^3.0",
    "silverstripe/asset-admin": "^1@dev",
    "silverstripe/cms": "^4@dev",
    "silverstripe/framework": "^4@dev",
    "silverware/colorpicker": "^1.0",
    "silverware/font-icons": "^1.0",
    "silverware/theme": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "SilverWare\\Components\\": "src/Components/",
      "SilverWare\\Core\\": "src/Core/",
      "SilverWare\\Crumbs\\": "src/Crumbs/",
      "SilverWare\\Dev\\": "src/Dev/",
      "SilverWare\\Extensions\\": "src/Extensions/",
      "SilverWare\\Folders\\": "src/Folders/",
      "SilverWare\\Forms\\": "src/Forms/",
      "SilverWare\\Grid\\": "src/Grid/",
      "SilverWare\\Lists\\": "src/Lists/",
      "SilverWare\\Model\\": "src/Model/",
      "SilverWare\\ORM\\": "src/ORM/",
      "SilverWare\\Sections\\": "src/Sections/",
      "SilverWare\\Security\\": "src/Security/",
      "SilverWare\\Tags\\": "src/Sections/",
      "SilverWare\\Tools\\": "src/Tools/",
      "SilverWare\\View\\": "src/View/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "installer-name": "silverware"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
